[bold cyan]usage[/bold cyan]:
  [bold]flag-kinds[/bold] [[flags]

[bold cyan]flags[/bold cyan]:
 [yellow]-a[/yellow]               kind: Command
 [yellow]-b[/yellow] [magenta]--bbbb[/magenta]        kind: InfixCommand
    [magenta]--cccc[/magenta]        kind: Stmt
 [yellow]-d[/yellow] [magenta]--dddd[/magenta]        kind: InfixStmt
 [yellow]-e[/yellow]        [faint]string[/faint] kind: Call
 [yellow]-f[/yellow] [magenta]--ffff[/magenta]        kind: InfixCallStmt
    [magenta]--gggg[/magenta] [faint]string[/faint] kind: CallStmt [faint](default: default)[/faint]
 [yellow]-h[/yellow] [magenta]--hhhh[/magenta]        kind: InfixCall
 [yellow]-i[/yellow]               kind: Stmt (but one line)
    [magenta]--help[/magenta]        print help